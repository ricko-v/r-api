{
    "builds": [
        {
            "src": "nuxt.config.js",
            "use": "@nuxtjs/vercel-builder"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/api",
            "dest": "api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "$1"
        }
    ]
}